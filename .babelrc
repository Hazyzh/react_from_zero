{
	"presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "ie > 7"]
      }
    }],
	"react",
	"stage-2"
	],
	"plugins": [[
    "transform-runtime",
    {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }
  ]]
 }
